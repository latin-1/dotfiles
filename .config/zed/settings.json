{
  "theme": "Catppuccin Latte",
  "ui_font_family": "Source Sans 3",
  "ui_font_size": 16,
  "buffer_font_family": "Iosevka Term",
  "buffer_font_size": 14,

  "vim_mode": true,
  "use_smartcase_search": true,
  "autosave": "on_focus_change",

  "terminal": {
    "shell": {
      "with_arguments": {
        "title_override": "Toolbox",
        "program": "toolbox",
        "args": ["enter"]
      }
    }
  },

  "proxy": "socks://127.0.0.1:1080"
}
